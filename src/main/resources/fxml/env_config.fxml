<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<Tab text="环境配置" xmlns:fx="http://javafx.com/fxml"
     fx:id="envConfig" fx:controller="com.huatai.controller.EnvConfigController">
  <GridPane>
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="305.0" minWidth="10.0" prefWidth="91.0"/>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="375.0" minWidth="10.0" prefWidth="375.0"/>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="98.0" minWidth="10.0" prefWidth="10.0"/>
    </columnConstraints>
    <rowConstraints>
      <RowConstraints maxHeight="13.0" minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="35.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="35.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="35.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="13.0" minHeight="5.0" prefHeight="7.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="35.0" minHeight="10.0" prefHeight="52.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="35.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="35.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
      <Label alignment="CENTER" prefHeight="46.0" prefWidth="129.0" text="个险" GridPane.rowIndex="1">
        <font>
          <Font name="Consolas" size="21.0"/>
        </font>
      </Label>
      <Label alignment="CENTER" layoutX="27.0" layoutY="10.0" prefHeight="46.0" prefWidth="129.0" text="EA"
             GridPane.rowIndex="5">
        <font>
          <Font name="Consolas" size="21.0"/>
        </font>
      </Label>
      <Label alignment="CENTER_RIGHT" layoutX="27.0" layoutY="10.0" prefHeight="46.0" prefWidth="129.0" text="test:"
             GridPane.rowIndex="2">
        <font>
          <Font name="Consolas Bold" size="16.0"/>
        </font>
      </Label>
      <Label alignment="CENTER_RIGHT" prefHeight="46.0" prefWidth="129.0" text="prod:" GridPane.rowIndex="3">
        <font>
          <Font name="Consolas Bold" size="16.0"/>
        </font>
      </Label>
      <Label alignment="CENTER_RIGHT" prefHeight="46.0" prefWidth="129.0" text="test:" GridPane.rowIndex="6">
        <font>
          <Font name="Consolas Bold" size="16.0"/>
        </font>
      </Label>
      <Label alignment="CENTER_RIGHT" prefHeight="46.0" prefWidth="129.0" text="prod:" GridPane.rowIndex="7">
        <font>
          <Font name="Consolas Bold" size="16.0"/>
        </font>
      </Label>
      <TextField fx:id="indivTestUrl" prefHeight="29.0" prefWidth="375.0" GridPane.columnIndex="1"
                 GridPane.rowIndex="2">
        <GridPane.margin>
          <Insets left="10.0" right="10.0"/>
        </GridPane.margin>
      </TextField>
      <TextField fx:id="indivProdUrl" prefHeight="29.0" prefWidth="375.0" GridPane.columnIndex="1"
                 GridPane.rowIndex="3">
        <GridPane.margin>
          <Insets left="10.0" right="10.0"/>
        </GridPane.margin>
      </TextField>
      <TextField fx:id="eaTestUrl" prefHeight="29.0" prefWidth="375.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
        <GridPane.margin>
          <Insets left="10.0" right="10.0"/>
        </GridPane.margin>
      </TextField>
      <TextField fx:id="eaProdUrl" prefHeight="29.0" prefWidth="375.0" GridPane.columnIndex="1" GridPane.rowIndex="7">
        <GridPane.margin>
          <Insets left="10.0" right="10.0"/>
        </GridPane.margin>
      </TextField>
      <Button fx:id="envConfigConfirm" mnemonicParsing="false" style="-fx-background-color: #e23b45;" text="保存"
              textFill="#f7f7f7" GridPane.columnIndex="1" GridPane.rowIndex="8" onAction="#setEnvConfig">
        <GridPane.margin>
          <Insets left="240.0"/>
        </GridPane.margin>
        <cursor>
          <Cursor fx:constant="HAND"/>
        </cursor>
      </Button>
      <Button fx:id="envConfigCancel" mnemonicParsing="false" style="-fx-background-color: green;" text="取消"
              textFill="WHITE" GridPane.columnIndex="1" GridPane.rowIndex="8" onAction="#resetEnvConfig">
        <GridPane.margin>
          <Insets left="300.0"/>
        </GridPane.margin>
        <cursor>
          <Cursor fx:constant="HAND"/>
        </cursor>
      </Button>
    </children>
  </GridPane>
</Tab>
